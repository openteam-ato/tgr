<div class="bottom_navigation">
  <div class="inner">

    <% object.values.first['children'].each_with_index do |hash, index| %>
      <div <%= 'class=last' if index == 4 %>>

        <h3>
          <%= render_link_for_navigation hash.last %>
        </h3>

        <ul>
          <% hash.last['children'].each do |name, item| %>
            <li>
              <%= render_link_for_navigation item %>
            </li>
          <% end if hash.last['children'] %>
        </ul>

      </div>
    <% end %>
  </div>
</div>
